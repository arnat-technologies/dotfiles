#!/bin/bash

SHELL_PATH=$DOTFILES/shell
export ALIASES_PATH=$SHELL_PATH/aliases

. $SHELL_PATH/lib/utils
. $SHELL_PATH/lib/api
. $SHELL_PATH/lib/file
. $SHELL_PATH/lib/print

case $(get-distro) in
    ubuntu|Darwin|arch) 
        . $ALIASES_PATH/_linux
    ;;
    ubuntu) 
        . $ALIASES_PATH/_ubuntu
    ;;
    Darwin)
        . $ALIASES_PATH/_osx
    ;;
    arch)
        . $ALIASES_PATH/_arch
    ;;
esac

